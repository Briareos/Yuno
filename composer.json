{
    "name":"symfony/framework-standard-edition",
    "description":"The \"Symfony Standard Edition\" distribution",
    "autoload":{
        "psr-0":{
            "":"src/"
        }
    },
    "require":{
        "php":">=5.3.3",
        "symfony/symfony":"2.1.*",
        "doctrine/orm":">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle":"1.0.*",
        "twig/extensions":"1.0.*@dev",
        "symfony/assetic-bundle":"2.1.*",
        "symfony/swiftmailer-bundle":"2.1.*",
        "symfony/monolog-bundle":"2.1.*",
        "sensio/distribution-bundle":"2.1.*",
        "sensio/framework-extra-bundle":"2.1.*",
        "sensio/generator-bundle":"2.1.*",
        "jms/security-extra-bundle":"1.2.*",
        "jms/di-extra-bundle":"1.1.*",
        "kriswallsmith/assetic":"1.1.*@dev",
        "mopa/bootstrap-bundle":"2.1.*",
        "zendframework/zend-paginator":"2.0.*",
        "stof/doctrine-extensions-bundle":"dev-master",
        "beberlei/DoctrineExtensions":"v0.1",
        "craue/twigextensions-bundle":"dev-master",
        "uco/twig-extensions-bundle":"dev-master"
    },
    "repositories":[
        {
            "type":"composer",
            "url":"http://packages.zendframework.com/"
        },
        {
            "type":"package",
            "package":{
                "name":"briareos/ajax-bundle",
                "version":"dev-master",
                "autoload":{
                    "psr-0":{
                        "Briareos\\AjaxBundle":""
                    }
                },
                "target-dir":"Briareos/AjaxBundle",
                "source":{
                    "url":"git@github.com:Briareos/BriareosAjaxBundle.git",
                    "type":"git",
                    "reference":"master"
                }
            }
        }
    ],
    "scripts":{
        "post-install-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config":{
        "bin-dir":"bin"
    },
    "minimum-stability":"dev",
    "extra":{
        "symfony-app-dir":"app",
        "symfony-web-dir":"web"
    }
}
